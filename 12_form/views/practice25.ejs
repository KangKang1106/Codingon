  <script>
    function call(name) {
      return new Promise(function(resolve) {
        setTimeout(function() {
          console.log('kim');
          resolve('kim');
        }, 1000);
      });
    }

    function back(name2) {
      return new Promise(function(resolve) {
        setTimeout(function() {
          console.log('back');
          resolve('back');
        }, 1000);
      });
    }

    function hell(cbHell) {
      return new Promise(function(resolve) {
        setTimeout(function() {
          resolve('callback hell');
        }, 1000);
      });
    }

    async function exec() {
      let name = await call();
      console.log(name + ' 반가워');
      let name2 = await back();
      console.log(name2 + ' 을 실행했구나');
      let cbHell = await hell();
      console.log('여기는 '+cbHell);
    }

    exec();
  </script>
